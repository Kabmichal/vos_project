
  <div class="col-md-12">
    <div class="well controls">
<div id="notes">
<h2><%= link_to "<", date: @date.prev_month %>
  <font color="black"><%= @date.strftime("%B %Y") %></font>
<%= link_to ">", date: @date.next_month %>
</h2>
<%= calendar @date do |date| %>
  <%= date.day %>
  <%if @notes_by_day[date] %>
    <ul>
    <% @notes_by_day[date].each do |note| %>
        <li><%=link_to note.note, note %></li>
        <%end %>
    </ul>
  <%end %>
<% end %>
</div>
    </div>
</div>
</aside>
<%= %>
  <span>
  <% if logged_in? %>
    <%= render 'notes/note_form' %>
  <% end %>
    </span>